      },
      "package": "com.sacrifice003.myapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "3d35c833-e90a-4895-a647-ca8290da7d06"
      }
    }
